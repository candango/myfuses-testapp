	<fuseaction name="test" class="TestAction" 
		path="actions/TestAction.class.php"/>
		
	
	<fuseaction name="header">
		<include template="dspHeader.php"/>       
    </fuseaction>
    
    <fuseaction name="footer">
		<include template="dspFooter.php"/>       
    </fuseaction>
		
</circuit>